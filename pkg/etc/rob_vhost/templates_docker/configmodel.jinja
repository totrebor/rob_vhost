<VirtualHost {{ vh_ip}}:80>
 ServerName {{ vh_hostname }}
 ServerAdmin support@progettosl.eu
 DocumentRoot {{ hostingDir}}
 ErrorLog ${APACHE_LOG_DIR}/{{ vh_code }}-{{ vh_hostname }}/error.log
 CustomLog ${APACHE_LOG_DIR}/{{ vh_code }}-{{ vh_hostname }}/access.log combined
#
# Alias "/.well-known" "/var/www/html/.well-known"
#
# <FilesMatch ".+\.ph(ar|p|tml)$">
#  <If "-f %{REQUEST_FILENAME}">
#   SetHandler "proxy:fcgi://php7fpm:9000"
#  </If>
# </FilesMatch>
#
# ProxyPreserveHost On
# ProxyRequests off
# AllowEncodedSlashes NoDecode
# ProxyPass /.well-known !
# ProxyPass / http://giteahost:3000/ nocanon
#
# Redirect permanent "/" "https://{{ vh_hostname }}"
</VirtualHost>

